					<span data-dismiss="modal" data-target="#Palindrome" class="close">X</span>

					<h3>Palindrome</h3>
					<div class="col-md-12">
						<pre class="brush:js">
	function palindromeFunc() {
		var palInput = document.getElementById("word").value;
		palInput = palInput.toLowerCase().replace(/[^a-z]+/g, ""); 
																		
		var iBox = "";	 
																					
		for (var i = palInput.length - 1; i >= 0; i--) {
			iBox += palInput.charAt(i); 
		}							
		if (iBox == palInput) {
		document.getElementById("result").innerHTML = "Your word " + palInput + " is Palindrome.";							
		}					
																	
		if (iBox != palInput) {
		document.getElementById("result").innerHTML = "Your word " + palInput + " is NOT Palindrome.";							
		}

	}
						</pre>
					</div>
					<div class="col-md-12">
						<div>
							<p style="font-size:35px">Type in your word to see if it's a Palindrome!!</p>
							<input id="word" type="text" style="font-size:40px" /><br />
							<button onclick="palindromeFunc()" style="width:180px;height:90px">Palindrome!!</button>
							<div class="col-md-12">
								<p id="result" style="font-size:75px !important"></p>
							</div>
						</div>
					</div>
				</div>
