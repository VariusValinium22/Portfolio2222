						<pre class="brush:js">
							function fizzBuzzNumber(){
					var x = document.getElementById("num27").value;
					var y = document.getElementById("num28").value;
					var output="";
					for (var i = 1; i <= 100; i++) {
					
						if (i % x === 0 && i % y === 0) {
							output += ' fizzBuzz ';

						} else if (i % x === 0) {
							output += ' fizz ';

						} else if (i % y === 0) {
							output += ' buzz ';

						} else {
							output += i;

						}

					}
						document.getElementById("result7").innerHTML = output;
				}
